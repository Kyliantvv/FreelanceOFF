{% extends "bundles/FOSUserBundle/layout.html.twig" %}

{% trans_default_domain 'FOSUserBundle' %}

{% block title %}{{ 'profile.title'|trans }}{% endblock %}

{% block navbar %}
    {% if app.user.hasRole(constant("App\\Enum\\RoleModeTypeEnum::ROLE_ENTERPRISE")) %}
        {% include 'Menu/Enterprise/navbar.html.twig' %}
    {% else %}
        {% include 'Menu/Freelance/navbar.html.twig' %}
    {% endif %}
{% endblock %}

{% block fos_user_content %}
    {% include "Profile/edit_content.html.twig" %}
{% endblock fos_user_content %}

{% block javascripts %}
    {{ encore_entry_script_tags('User/profile') }}
{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('User/profile') }}
{% endblock %}
